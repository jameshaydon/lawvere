ob Base ListI =
  [ empty: {},
    cons:  { head: Int, tail: ListI }
  ]

ar sumList : ListI --> Int =
  [ empty = 0,
    cons  = .head + .tail sumList] 

ar example : {} --> ListI =
  empty.
  { head = 10, tail = } cons.
  { head = 20, tail = } cons.
  { head = 30, tail = } cons.

ar main : {} --> Int =
  example sumList
